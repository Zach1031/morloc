@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .

# This config file is based on stackoverflow #30217704 by Cyril

<> rdf:type         ja:RDFDataset ;
    ja:graph :inf .

:inf rdf:type ja:InfModel ;
    ja:baseModel :data ;
    ja:reasoner
        [ ja:reasonerURL <http://jena.hpl.hp.com/2003/GenericRuleReasoner> ;
          ja:rulesFrom <file:/home/z/src/git/morloc/logic/law.rules> 
        ] ;
    .

:data rdf:type ja:MemoryModel ;
    ja:content [ja:externalContent <input-data.ttl>] ;
    .
