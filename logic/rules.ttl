@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mlc: <http://www.morloc.io/ontology/000/> .

# Typeclass declarations

mlc:TypeClass rdfs:type owl:Class . 
mlc:Functor rdfs:subClassOf mlc:TypeClass . 

# Type declarations

mlc:Type                 rdf:type        owl:Class   .
mlc:ParameterizedType    rdfs:subClassOf mlc:Type .
mlc:ParameterizedGeneric rdfs:subClassOf mlc:Type .
mlc:AtomicType           rdfs:subClassOf mlc:Type .
mlc:AtomicGeneric        rdfs:subClassOf mlc:Type .

mlc:Function rdfs:subClassOf mlc:Type .

mlc:Constraint rdf:type owl:Class .

mlc:hasConstraint rdfs:type owl:ObjectProperty ;
                  rdfs:domain mlc:Function ;
                  rdfs:range mlc:Constraint .

mlc:Real rdfs:subClassOf mlc:AtomicType .
mlc:Bool rdfs:subClassOf mlc:AtomicType .
mlc:Text rdfs:subClassOf mlc:AtomicType .

# Records and tuples are neither functors nor collections
mlc:Record rdfs:subClassOf mlc:ParameterizedType  .
mlc:Tuple  rdfs:subClassOf mlc:ParameterizedType .

mlc:Collection rdfs:subClassOf mlc:ParameterizedType ; 
               mlc:property mlc:Functor .

rdfs:Seq rdfs:subClassOf mlc:Collection .
rdfs:Bag rdfs:subClassOf mlc:Collection .

mlc:List rdfs:subClassOf rdfs:Seq .

mlc:Script rdfs:subClassOf rdfs:Bag .

mlc:Call rdfs:type owl:Class .

mlc:Lhs owl:inversOf mlc:Rhs ;
        owl:disjoinWith mlc:Rhs .

mlc:keyOf rdf:type owl:FunctionalProperty .
