@type
foo :: Int -> void
bar :: void -> Int
print :: * -> void

@path
foo . bar

@arg
bar :: x=1 y=2

@lang
* :: py

@cache
foo :: memcache

@check
foo :+ true
foo :+ true

@0
foo :+ print . "0"
foo :+ print . "0"

@1
foo :+ print . "1"
foo :+ print . "1"

@2
foo :+ print . "- 2"
foo :+ print . ". 2"

@3
foo :+ print . "- 3"
foo :+ print . ". 3"

@4
foo :+ print . "- - 4"
foo :+ print . ". . 4"

@5
foo :+ print . "- - 5"
foo :+ print . ". . 5"

@6
foo :+ print . "- - 6"
foo :+ print . ". . 6"

@7
foo :+ print . "- - 7"
foo :+ print . ". . 7"

@8
foo :+ print . "- 8"
foo :+ print . ". 8"

@9
foo :+ print . "- 9"
foo :+ print . ". 9"

@arg
print :: file=sys.stderr


@include
core/memcache
core/type

@source py

def true():
    return True

def foo(x):
    pass

def bar(x=None, y=None):
    return x + y
