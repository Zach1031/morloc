@include
core/type

@type
seq :: Int -> Int -> [Int]
max :: [Int] -> Int
sqrt :: Int -> Int # TODO get real
print_na :: ? -> String
is_positive :: Int -> Bool

@source R

is_positive <- function(x){ all(x >= 0) }
print_na <- function(...){ "na" }

@export
sqrt

@path
sqrt . max . seq . -1 -10

@lang
* :: R

@check
sqrt := is_positive . <max>

@fail
sqrt :: print_na
