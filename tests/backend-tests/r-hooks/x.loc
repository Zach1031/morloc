@include
core/memcache
core/type
core/atomic

@type
bar :: void -> Int
foo :: Int -> void
cat :: String -> void

@path
foo . bar

@source R
foo <- function(x, y=0) { NULL }
bar <- function() { 1 }

@arg
foo :: y=1
cat :: file="/dev/stderr"

@lang
* :: R

@cache
foo :: memcache

@check
foo :: true

@0
foo :: cat . "0\n"

@1
foo :: cat . "1\n"

@2
foo :: cat . "- 2\n"

@3
foo :: cat . "- 3\n"

@4
foo :: cat . "- - 4\n"

@5
foo :: cat . "- - 5\n"

@6
foo :: cat . "- - 6\n"

@7
foo :: cat . "- - 7\n"

@8
foo :: cat . "- 8\n"

@9
foo :: cat . "- 9\n"
