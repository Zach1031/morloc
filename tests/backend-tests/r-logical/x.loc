@include
"core/R/atomic.loc"

@source R
is_a <- function() { TRUE  }
is_b <- function() { TRUE  }
is_c <- function() { FALSE }
is_d <- function() { FALSE }

say_hi <- function() { cat("hi\n") }
say_bi <- function() { cat("bi\n") }

@lang
* :: R

@path
say_hi say_bi

@check
say_hi :: and . is_a (any . is_b is_c (not . is_d))
say_bi :: not . and . is_a (any . is_b is_c (not . is_d))
