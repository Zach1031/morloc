@include
core/datcache
core/atomic

@source sh

foo () {
    echo -n
}
bar () {
    echo 1
}
echoerr () {
    echo $1 > /dev/stderr
}

@path
foo . bar

@arg
foo :: 1

@lang
* :: sh

@cache
foo :: datcache

@check
foo :: true

@0
foo :: echoerr . "0"

@1
foo :: echoerr . "1"

@2
foo :: echoerr . "- 2"

@3
foo :: echoerr . "- 3"

@4
foo :: echoerr . "- - 4"

@5
foo :: echoerr . "- - 5"

@6
foo :: echoerr . "- - 6"

@7
foo :: echoerr . "- - 7"

@8
foo :: echoerr . "- 8"

@9
foo :: echoerr . "- 9"
