@comment

This code is equivalent to

man ls | grep -Po "[a-z]+ed" | sort | uniq

@type
has_man :: String -> Bool
man :: String -> [String]
grep :: [String] -> [String]
sort :: [String] -> [String]
uniq :: [String] -> [String]

@path
uniq . sort . grep . man . "ls"

@arg
grep := -Po -e "[a-z]+ed"
man := --no-hyphenation

@lang
* :: sh

@cache
sort :: datcache
man :: datcache

@assert
man := has_man . "ls"

@include
core/datcache

@source sh

has_man () {
    man $1 &> /dev/null && echo true || echo false
}
