@type
tup :: Void -> (Int, (Int, [Int]))


@path
tup:py tup:sh tup:R


@export
tup:py as py
tup:sh as sh
tup:R  as r


@lang
tup:py :: py
tup:sh :: sh
tup:R :: R


@source py
def tup():
    return (1, (2, [3,4,5]))

@source sh
tup (){
    echo -e "oh poop"
}

@source R
tup <- function(){
    list(1, list(2, c(3,4,5)))
}
