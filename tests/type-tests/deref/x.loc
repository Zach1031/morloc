@path sh
A :: *Foo
B :: *Foo
C :: *Foo

Foo :: echo . &(id . $1) 7

@export
A/echo as py
B/echo as sh
C/echo as r

@lang
A/id :: py
B/id :: R
C/id :: sh

@alias
A/id :: print
B/id :: cat
C/id :: echo
