import rbase (id, Map)

source R ("list" as Person)

data (Person a) = Person {name :: Str, info :: a}
data R (Person a) = Person {name :: "character", info :: a}

export foo

-- This function returns the input, but passes it though a language-specific id
-- function, which forces deserialization and then serialization.
foo :: Person (Map Str Str) -> Person (Map Str Str)
foo r :: Person (Map "character" "character") -> Person (Map "character" "character")
foo xs = id xs
