source Cpp from "math.hpp" ("sqrt");
source Cpp from "doubleSerial.hpp" ("packDouble", "unpackDouble");
source R ("log");
source R from "minimal.R" ("packGeneric", "unpackGeneric");

packDouble Cpp :: packs => "double" -> "char*";
unpackDouble Cpp :: unpacks => "char*" -> "double";

sqrt :: Num -> Num;
sqrt Cpp :: "double" -> "double";

packGeneric R :: packs => forall a . a -> JSON;
unpackGeneric R :: unpacks => forall a . JSON -> a;

log :: Num -> Num;
log R :: "numeric" -> "numeric";

export r2cpp
r2cpp x = log (sqrt x);
