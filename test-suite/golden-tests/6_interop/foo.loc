source R ("rbinom", "mean");
import rbase;
import math (fibonacci, sqrt);

export mean;
export rbinom;
export fibonacci;
export sqrtfibmean;

rbinom :: Num -> Num -> Num -> Num;
rbinom R :: n:integer -> size:integer -> p:numeric -> vector integer
    where {0 <= p <= 1};

mean :: [Num] -> Num;
mean R :: vector numeric -> numeric;

sqrtfibmean n = sqrt (mean (fibonacci n));
