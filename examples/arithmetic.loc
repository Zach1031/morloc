-- import rbase (sqrt);

source "R" ("sqrt", "sum");

export rms;

mul :: Num -> Num -> Num;

square :: Num -> Num;
square x = mul x x;

map :: forall a b . (a -> b) -> [a] -> [b];

sqrt :: Num -> Num;

sum :: [Num] -> Num;
sum R :: "numeric" -> "numeric";

rms :: [Num] -> Num;
rms xs = sqrt (sum (map square xs));
