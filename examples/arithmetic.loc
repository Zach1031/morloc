import rbase (map, mul);

source R ("sqrt", "sum", "abs");

export rms;
export abssum;

square :: Num -> Num;
square x = mul x x;

sqrt :: Num -> Num;
sqrt R :: "numeric" -> "numeric";

sum :: [Num] -> Num;
sum R :: "numeric" -> "numeric";

abs :: Num -> Num;
abs R :: "numeric" -> "numeric";

rms :: [Num] -> Num;
rms xs = sqrt (sum (map square xs));

abssum :: [Num] -> Num;
abssum xs = sum (map abs xs);
