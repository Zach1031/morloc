source "R" ("sum", "runif")
source "R" ("runif")
source "R"
    from
    "minimal.R"
    ("packGeneric", "unpackGeneric")

export sum;
export rand;
export rsum;

packGeneric R :: packs => a -> "character";
unpackGeneric R :: unpacks => "character" -> a;

sum :: [Num] -> Num;

runif :: Int, Num, Num -> [Num];

rand :: Int -> [Num];
rand n = runif n 5 6;

rsum :: Int -> Num;
rsum n = sum (rand n);
