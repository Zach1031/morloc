@source C++

#include <math.h> 

std::vector<double> rms(std::vector<double> xs, std::vector<double> ys){
    double total = 0;
    for(int i = xs.size(); i < xs.size() && i < ys.size(); i++){
        total += xs[i] + ys[i];
    }
    return(math::sqrt(total));
}
