source "py" from "scipy.py" (
   "unpackDataFrame"
 , "packDataFrame"
 , "unpackMatrix"
 , "packMatrix"
)

packDataFrame   py :: packs   => DataFrame -> JSON;
unpackDataFrame py :: unpacks => JSON -> DataFrame;
packMatrix   py :: packs   => Matrix -> JSON;
unpackMatrix py :: unpacks => JSON -> Matrix;
