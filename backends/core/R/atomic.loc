@type

# and ::  Bool  -> Bool -> Bool
# all :: [Bool] -> Bool
# or  ::  Bool  -> Bool -> Bool
# any :: [Bool] -> Bool
# not ::  Bool  -> Bool

@source R

null <- function(...) {
    list(...) # to force evaluation of inputs
    NULL      # but return nothing
}

and <- function(a, b) { a && b }

or  <- function(a, b) { a || b }

not <- function(a) { ! a }
