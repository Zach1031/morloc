@source bash

# String -> Bool
manifold_exists() {
    type $1 | grep -q 'function'
}

# Text -> NULL
null() {
    cat > /dev/null
}

# String -> Bool
datcache_chk() {
    test -d cache && test -f cache/$1.dat
}

# NULL -> Text
datcache_get() {
    cat cache/$1.dat
}

# Text -> Text
datcache_put() {
    tee > cache/$1.dat
}

id() {
    cat
}

# (a -> b) -> c -> a -> Text
run() {
    $@
}

nothing() {
    echo -n
}
