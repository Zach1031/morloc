@source bash

manifold_exists() {
    type $1 | grep -q 'function'
}

null() {
    $1 $@ > /dev/null
}

datcache_chk() {
    test -d cache && test -f cache/$1.dat
}

datcache_get() {
    cat cache/$1.dat
}

datcache_put() {
    tee > cache/$1.dat
}

id() {
    cat $1
}

run() {
    $@
}

nothing() {
    echo -n
}
